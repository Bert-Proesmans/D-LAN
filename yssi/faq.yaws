<erl>
   -import(d_lan_lang, [tr/3, tr/4]).

   out(A) ->
      {ehtml,
         lists:map(
            fun(N) ->
               {'div', [{class, "qa"}], [
                  {'div', [{class, "question"}], {em, [], tr(faq, list_to_atom("q" ++ integer_to_list(N)), A)}},
                  {'div', [{class, "answer"}], tr(faq, list_to_atom("a" ++ integer_to_list(N)), A)}
               ]}
            end,
            lists:seq(1, 5)
         )
      }.
</erl>
